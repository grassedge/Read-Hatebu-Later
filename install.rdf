<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
    <em:version>0.1.1</em:version><!-- アドオンのバージョン -->
    <em:id>rhl@shibahata.net</em:id><!-- アドオンのid -->
    <em:name>Read Hatebu Later</em:name>
    <em:type>2</em:type>
    <em:optionsURL>chrome://rhl/content/config.xul</em:optionsURL>
    <em:iconURL></em:iconURL><!-- アイコンのChromeURL -->
    <em:targetApplication>
      <!-- Firefox -->
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>4.0.*</em:minVersion><!--対応するFirefoxの最低バージョン-->
        <em:maxVersion>9.*</em:maxVersion><!--対応するFirefoxの最高バージョン-->
      </Description>
    </em:targetApplication>

    <em:localized>
      <Description>
        <em:locale>en-US</em:locale>
        <em:creator>shibahata</em:creator>
        <em:name>Read Hatebu Later</em:name>
        <em:description>add a page to Hatena bookmark by 1 click.</em:description>
        <em:homepageURL>https://github.com/hatz48/Read-Hatebu-Later/</em:homepageURL>
      </Description>
    </em:localized>
    <em:localized>
      <Description>
        <em:locale>ja</em:locale><!-- 日本語環境での情報であることを表す。 -->
        <em:creator>芝端</em:creator><!-- 日本語の作者名 -->
        <em:name>あとで読ブ</em:name><!-- 日本語のアドオン名 -->
        <em:description>1クリックではてブに追加する拡張です</em:description><!-- 日本語説明 -->
        <em:homepageURL>https://github.com/hatz48/Read-Hatebu-Later/</em:homepageURL><!--ホームページ　URL -->
      </Description>
    </em:localized>
  </Description>
</RDF>
